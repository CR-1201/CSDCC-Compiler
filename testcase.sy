// Loops will be fused, since both of them iterate from [0, n) and they
// do not modify the same array.
int main() {
  int A[100] = {0};
  int B[100] = {0};
  int n = 100;
  int i = 0;
  while (i < n) {
    A[i] = i;
    i = i + 1;
  }
  i = 0;
  while (i < n) {
    B[i] = i * 2;
    i = i + 1;
  }
  return B[99];
}